{
  "name": "lofi-player",
  "version": "1.0.0",
  "description": "A Lofi Player builded with HTML, CSS and Javascript using Parcel as Module Bundler",
  "main": "index.js",
  "scripts": {
    "dev": "parcel public/index.html --out-dir development -p 3000",
    "pre-deploy": "parcel build public/index.html --out-dir dist --public-url /lofi-player",
    "deploy": "node publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LaksCastro/lofi-player.git"
  },
  "keywords": [
    "parcel"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bradtraversy/vanilla-parcel-boilerplate/issues"
  },
  "homepage": "https://lakscastro.github.io/lofi-player",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/runtime-corejs2": "^7.9.2",
    "parcel-bundler": "^1.12.4",
    "sass": "^1.26.3"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "gh-pages": "^2.2.0"
  }
}
